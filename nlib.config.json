{
    "app": {
        "name": "NLib Web Server Application",
        "version": "2.0.0",
        "updated": "2019-08-12 02:00"
    },
    "webserver": {
        "port": 3000,
        "websocket": {
            "enable": false
        },
        "monitor": {
            "enable": false
        },
        "swagger": {
            "enable": false,
            "apis": [
                "./server.js"
            ]
        },
        "cookies": {
            "secret": "YOUR_SECURE_KEY@123"
        },
        "favicon": {
            "path": "public",
            "fileName": "favicon.ico"
        },
        "public": {
            "paths": [
                {
                    "route": "/public",
                    "path": "public",
                    "maxAge": "30s",
                    "enable": true
                },
                {
                    "route": "/dist/js",
                    "path": "public/dist/jquery-3.3.1",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist/css",
                    "path": "public/dist/jquery-ui-1.12.1",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist/js",
                    "path": "public/dist/jquery-ui-1.12.1",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist/js",
                    "path": "public/dist/popperjs-1.15.0",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist",
                    "path": "public/dist/bootstrap-4.2.1",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist",
                    "path": "public/dist/font-awesome-5.9.0",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist/css",
                    "path": "public/dist/emoji-symbols-1.0.0",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist",
                    "path": "public/dist/flag-icon-css-3.1.0",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist/js",
                    "path": "public/dist/riotjs-3.13.2",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/components",
                    "path": "dist/component/riot",
                    "maxAge": "5y",
                    "enable": true
                },
                {
                    "route": "/dist",
                    "path": "public/lib",
                    "maxAge": "15s",
                    "enable": true
                }
            ]
        }
    }
}